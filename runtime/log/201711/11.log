INSERT INTO uee_article(`title`, `type`, `slug`, `content`, `keyword`, `description`, `cate_id`, `img_id`, `author`, `sources`, `read`, `comment`, `stick`, `create_time`, `update_time`) VALUES ('工业4.0和精益生产密切相关', '1', '', '<p style=\"margin-top: 0px;margin-bottom: 0px;padding: 0px;color: rgb(102, 102, 102);font-family: tahoma, &#39;microsoft yahei&#39;, 微软雅黑, 宋体;font-size: 12px;white-space: normal;background-color: rgb(255, 255, 255)\"><span style=\";padding: 0px\">在国家制造大战略的鼓舞下，国内已有很多企业开始尝试工业自动化以及工业</span>4.0<span style=\";padding: 0px\">的技术开发和产品布局，以图在未来中国制造逐步走向工业</span><span style=\";padding: 0px\">4.0</span><span style=\";padding: 0px\">的升级改造中获得商业机会。最近，网络上也开始传播海尔、格力等企业关于工业</span><span style=\";padding: 0px\">4.0</span><span style=\";padding: 0px\">示范工厂的视频，说明工业</span><span style=\";padding: 0px\">4.0</span><span style=\";padding: 0px\">离我们并不遥远，德国制造能办到的事，中国制造也能办到。但就中国大量的制造型企业特别是中小企业来说，管理现状依然停留在工业&nbsp;</span><span style=\";padding: 0px\">1.5</span><span style=\";padding: 0px\">、工业&nbsp;</span><span style=\";padding: 0px\">2.0&nbsp;</span><span style=\";padding: 0px\">的低水平上，工业</span><span style=\";padding: 0px\">4.0</span><span style=\";padding: 0px\">还是一个模糊的和不确定的未来。 &nbsp;</span></p><p style=\"margin-top: 0px;margin-bottom: 0px;padding: 0px;color: rgb(102, 102, 102);font-family: tahoma, &#39;microsoft yahei&#39;, 微软雅黑, 宋体;font-size: 12px;white-space: normal;background-color: rgb(255, 255, 255)\"><br/></p><p style=\"margin-top: 0px;margin-bottom: 0px;padding: 0px;color: rgb(102, 102, 102);font-family: tahoma, &#39;microsoft yahei&#39;, 微软雅黑, 宋体;font-size: 12px;white-space: normal;background-color: rgb(255, 255, 255)\"><span style=\";padding: 0px\">企业走向工业</span>4.0<span style=\";padding: 0px\">，需要逐步进行精益化、自动化、智慧化（信息化的高级形式）、智能化等四化建设，而精益化是其他三化的基础，精益化将伴随企业走向工业</span><span style=\";padding: 0px\">4.0</span><span style=\";padding: 0px\">的整个进程。可见，工业</span><span style=\";padding: 0px\">4.0</span><span style=\";padding: 0px\">是目标，精益化既是走向工业</span><span style=\";padding: 0px\">4.0</span><span style=\";padding: 0px\">的手段，也是一个持续的过程。其实工业</span><span style=\";padding: 0px\">4.0</span><span style=\";padding: 0px\">本身并不可怕，真正可怕的是，我们的企业停止了进化，或者进化得太慢，以至于生存出现问题。如何做到快速进化，让进化速度能够契合企业生存的需要？答案是走精益之路，即开始拥有精益思维，积极采取精益行动。</span></p><p><br/></p>', '工业4.0和精益生产密切相关', '工业4.0和精益生产密切相关', '0', '0', '', '', '0', '1', '0', '1510348147', '1510348147')
