INSERT INTO uee_product(`title`, `type`, `slug`, `price`, `sales`, `content`, `keyword`, `description`, `cate_id`, `comment`, `stick`, `img_id`, `imgs`, `create_time`, `update_time`) VALUES ('安灯呼叫系统', '1', 'andonsystem', '0', '0', '<p>Andon系统主要功能：</p><p>1、工位作业管理——工位呼叫;集中事件呼叫。</p><p>2、设备运行管理——故障、运行状态、维护信息。</p><p>3、信息可视管理——通过Andon看板，显示呼叫信息、故障信息、停线信息。</p><p>4、物料呼叫——通过物料显示屏，显示物料呼叫信息。</p><p>5、质量呼叫——通过广播，呼叫质量信息。</p><p>6、设备呼叫——当设备故障时，通过广播进行呼叫。</p><p>7、维修呼叫管理——通过维修Andon看板，显示维修信息。</p><p>8、公共信息管理——通过信息显示屏，显示各种公共信息。安灯系统的作用及其使用</p><p><br/></p>', '安灯呼叫系统,andon系统,安灯看板,电子看板', 'Andon系统能够收集生产线上有关设备和质量管理等与生产有关的信息，加以处理后，控制分布于车间各处的灯光和声音报警系统。从而实现生产信息的透明化。', '3', '1', '0', '184', '184,185,186,187,188', '1510347961', '1510347961')
